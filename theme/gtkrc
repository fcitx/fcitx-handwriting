style "button_0"
{ 

	engine "pixmap"
	{
	# Normal button
	image 
	{
		function		= BOX
		state			= NORMAL
		shadow			= OUT
		file			= "Buttons/1.png"
		border			={ 4, 4, 4, 4 }
		stretch			= TRUE
	}
	# Pressed
	image
	{
		function		= BOX
		state			= ACTIVE
		file			= "Buttons/2.png"
		border			= { 4, 4, 4, 4 }
		stretch			= TRUE
	}
	#PRELIGHT
	image
	{
		function		= BOX
		state			= PRELIGHT
		file			= "Buttons/2.png"
		border			= { 4, 4, 4, 4 }
		stretch			= TRUE
	}
	image
        {
                function                = BOX
                state                   = INSENSITIVE
#                shadow                  = IN
                file                    = "Buttons/2.png"
                border                  = { 4, 4, 4, 4 }
                stretch                 = TRUE
        }

    }
}
